{
  "name": "ws-flare-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config proxy.conf.json",
    "start:server": "ng run unicron-ui:server",
    "build": "ng build --prod --progress false && ng run unicron-ui:server --progress false && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress false --colors",
    "test": "jest --ci --silent",
    "lint": "ng lint",
    "e2e:graphql": "ts-node -O '{\"module\": \"commonjs\"}' cypress/mocks/server.ts",
    "e2e:server": "ng serve --port=5000",
    "e2e:graphql:start": "start-test e2e:graphql http-get://localhost:5000 e2e:tests",
    "e2e:tests": "cross-env CYPRESS_baseUrl=http://localhost:5000 cypress open",
    "e2e": "start-test e2e:server http-get://localhost:5000 e2e:graphql:start",
    "e2e:ci:server": "npm run build --configuration=e2e && node dist/server",
    "e2e:ci:graphql:start": "start-test e2e:graphql http://localhost:4000 e2e:ci:test",
    "e2e:ci:test": "cross-env CYPRESS_baseUrl=http://localhost:4000 cypress-run",
    "e2e:ci": "start-server-and-test e2e:ci:server http://localhost:4000 e2e:ci:graphql:start",
    "test:watch": "jest --watch --logHeapUsage",
    "kube": "telepresence --swap-deployment test-unicron-ui --run yarn start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/platform-server": "^7.0.2",
    "@angular/router": "~7.0.0",
    "@davinkevin/jest": "^0.1.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "apollo-angular": "^1.5.0",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-ws": "^1.0.10",
    "core-js": "^2.5.4",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "highcharts": "^7.0.3",
    "highcharts-angular": "^2.4.0",
    "monaco-editor": "^0.15.6",
    "ngrx-store-localstorage": "^5.1.0",
    "ngrx-store-logger": "^0.2.2",
    "ngx-contextmenu": "^5.1.1",
    "ngx-monaco": "^0.8.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "subscriptions-transport-ws": "^0.9.15",
    "xterm": "^3.10.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^22.2.0",
    "@types/node": "~8.9.4",
    "@types/xterm": "^3.0.0",
    "apollo-angular-link-http": "^1.4.0",
    "apollo-link-http": "^1.5.9",
    "apollo-server": "^2.2.6",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "cypress": "3.1.5",
    "cypress-run": "^1.0.6",
    "faker": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-marbles": "^2.0.0",
    "jest-preset-angular": "^5.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "start-server-and-test": "^1.7.11",
    "ts-loader": "^5.3.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  }
}
